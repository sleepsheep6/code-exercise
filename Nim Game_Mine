public class Solution {
    public boolean canWinNim(int n) {
        boolean res=false;
        if(n>0){
           
            boolean a1=true,a2=true,a3=true,a4=false;
            if(n%4==1) res = a1;
            if(n%4==2) res = a2;
            if(n%4==3) res = a3;
            if(n%4==0) res = a4;
            
        }
        return res;
    }
}

/**
fn: I'm able to take the nth step;
f1: true;
f2: true;
f3: true;
f4: true;

fn=f(n-4)
**/
